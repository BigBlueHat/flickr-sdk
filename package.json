{
  "name": "flickr-sdk",
  "version": "3.0.0-alpha.1",
  "description": "Almost certainly the best Flickr API client in the world for node and the browser",
  "keywords": [
    "flickr",
    "api",
    "sdk",
    "photos",
    "search"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "request.js",
    "plugins",
    "services",
    "validate.js"
  ],
  "scripts": {
    "reflect": "node script/reflect",
    "build": "node script/build > services/rest.js",
    "build-tests": "node script/build-tests",
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/flickr/flickr-sdk.git"
  },
  "author": [
    "Jeremy Ruppel <ruppel@yahoo-inc.com>",
    "Alex Seville <seville@yahoo-inc.com>",
    "Phil Dokas <pdokas@yahoo-inc.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 4"
  },
  "publishConfig": {
    "tag": "next"
  },
  "devDependencies": {
    "ejs": "~2.5.2",
    "eslint": "~2.13.1",
    "eslint-config-flickr": "~1.3.1",
    "mocha": "~3.0.2",
    "nock": "~8.0.0",
    "nyc": "^10.3.2",
    "require-dir": "~0.3.1",
    "sinon": "~1.17.6"
  },
  "dependencies": {
    "deepmerge": "^1.1.1",
    "superagent": "^3.5.2"
  },
  "bugs": {
    "url": "https://github.com/flickr/flickr-sdk/issues"
  },
  "homepage": "https://github.com/flickr/flickr-sdk#readme"
